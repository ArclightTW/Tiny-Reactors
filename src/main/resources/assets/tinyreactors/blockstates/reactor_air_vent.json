{
	"multipart"				: [
		{
			"apply"			: {
				"model"		: "tinyreactors:air_vent/frame"
			}
		},
		
		{
			"when"			: { "vent" : "empty" },
			"apply"			: {
				"model"		: "tinyreactors:air_vent/fan_empty"
			}
		},
		
		{
			"when"			: { "vent" : "wood|stone|iron|gold|diamond", "operational" : "false" },
			"apply"			: {
				"model"		: "tinyreactors:air_vent/fan_closed"
			}
		},
		
		{
			"when"			: { "vent" : "wood", "operational" : "true" },
			"apply"			: {
				"model"		: "tinyreactors:air_vent/fan_wood"
			}
		},
		
		{
			"when"			: { "vent" : "stone", "operational" : "true" },
			"apply"			: {
				"model"		: "tinyreactors:air_vent/fan_stone"
			}
		},
		
		{
			"when"			: { "vent" : "iron", "operational" : "true" },
			"apply"			: {
				"model"		: "tinyreactors:air_vent/fan_iron"
			}
		},
		
		{
			"when"			: { "vent" : "gold", "operational" : "true" },
			"apply"			: {
				"model"		: "tinyreactors:air_vent/fan_gold"
			}
		},
		
		{
			"when"			: { "vent" : "diamond", "operational" : "true" },
			"apply"			: {
				"model"		: "tinyreactors:air_vent/fan_diamond"
			}
		}
	]
}